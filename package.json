{
  "name": "@nicolascribbles/sketchbook-ui",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/demo",
    "build:lib": "npm run build --workspace=packages/library",
    "build:demo": "npm run build --workspace=packages/demo",
    "build": "npm run build:lib && npm run build:demo"
  }
}
